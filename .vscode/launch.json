{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 52015,
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "PowerShell",
      "request": "launch",
      "name": "ps: Interactive Session",
      "cwd": "${workspaceRoot}"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    },

  ]
}
